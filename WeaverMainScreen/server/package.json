{
  "name": "weaver-main-server",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run gen:types && tsx watch src/index.ts",
    "build": "npm run gen:types && tsc -p .",
    "start": "node dist/index.js",
    "selftest": "tsx scripts/selftest-runner.ts",
  "debug:cors": "node ./scripts/print-cors.mjs",
    "gen:types": "node ./scripts/gen-contracts.mjs",
    "ci:env": "tsx scripts/ci-env-check.ts",
    "ci:models": "tsx scripts/ci-model-registry.ts",
    "ci:smoke": "tsx scripts/ci-smoke.ts",
    "ci:all": "npm run ci:env && npm run build && npm run ci:models && npm run ci:smoke"
  },
  "scriptsExtra": {
    "integration": "tsx scripts/integration-test.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "google-auth-library": "^9.14.1",
    "openai": "^5.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "openapi-typescript": "^7.6.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
